{
  "name": "@swimlane/cy-dom-diff",
  "version": "0.0.0",
  "description": "matching chunks of DOM against HTML; including dynamic content.",
  "main": "index.js",
  "module": "index.js",
  "typings": "src/types/main.d.ts",
  "files": [
    "lib",
    "src",
    "index.js",
    "commands.js"
  ],
  "scripts": {
    "clean": "trash lib ./*.js",
    "build": "npm run clean && tsc",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s test:*",
    "test:lint": "tslint --project . && prettier \"src/**/*.ts\" --list-different",
    "test:unit": "cypress run",
    "open": "cypress open",
    "version": "chg release -y && git add -A CHANGELOG.md",
    "np": "np && npm run clean",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "author": "J. Harshbarger",
  "license": "MIT",
  "dependencies": {
    "@open-wc/semantic-dom-diff": "^0.18.0",
    "diff": "^4.0.2",
    "strip-indent": "^3.0.0"
  },
  "devDependencies": {
    "@types/diff": "^4.0.2",
    "@types/faker": "^5.1.2",
    "chg": "^0.4.0",
    "cypress": "^5.2.0",
    "faker": "^5.1.0",
    "np": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "trash-cli": "^3.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "cypress": "^5.2.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/cy-dom-diff.git"
  },
  "keywords": [
    "cypress",
    "commands",
    "dom-diff",
    "testing"
  ],
  "bugs": {
    "url": "https://github.com/swimlane/cy-dom-diff/issues"
  },
  "homepage": "https://github.com/swimlane/cy-dom-diff#readme"
}
