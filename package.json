  },
  "devDependencies": {
    "@types/faker": "^5.1.2",
    "chg": "^0.4.0",
    "cypress": "^5.2.0",
    "faker": "^5.1.0",
    "np": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
